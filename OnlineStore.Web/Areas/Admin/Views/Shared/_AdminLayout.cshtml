<!DOCTYPE html>
<html lang="en">
<head>
    <partial name="_MetaStyle" />
    <title>محصولات منطقه آزاد انزلی | @ViewData["Title"]</title>
    <partial name="_LinkStyle" />
</head>

<body class="light rtl">
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
            <div class="m-t-30">
                <img class="loading-img-spin" src="/admin-assets/images/Logo/Logo.png" alt="admin">
            </div>
            <p>لطفا صبر کنید...</p>
        </div>
    </div>
    <!-- #END# Page Loader -->
    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>
    <!-- #END# Overlay For Sidebars -->
    <!-- Top Bar -->
    <nav class="navbar">
        <div class="container-fluid">
            <partial name="_Navbar" />
        </div>
    </nav>
    <!-- #Top Bar -->
    <div>
        <!-- Left Sidebar -->
        <aside id="leftsidebar" class="sidebar">
            <!-- Menu -->
            <div class="menu">
                <ul class="list">
                    <partial name="_LeftSidebar" />
                </ul>
            </div>
            <!-- #Menu -->
        </aside>
        <!-- #END# Left Sidebar -->
        <!-- Right Sidebar -->
        <aside id="rightsidebar" class="right-sidebar">
            <partial name="_RightSideBar" />
        </aside>
        <!-- #END# Right Sidebar -->
    </div>
    <section class="content">
        <div class="container-fluid">
            @RenderBody()
        </div>
    </section>
    <partial name="_Scripts" />
    <partial name="_SiteNotification" />
    @RenderSection("Scripts", false)
</body>
</html>